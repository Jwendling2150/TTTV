   </footer>\n</div>","speaker":{"scene":"D0AFWl0vPlDj9Sq9","token":"stvwUcBDBONuhZV4","actor":"CGXjsdcHOy6mNXa1","alias":"Ika"},"whisper":[],"blind":false,"emote":false,"flags":{"core":{"canPopout":true}},"_id":"puYG1a6UzBIxCTGh"}
